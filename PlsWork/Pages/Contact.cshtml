@page
@model PlsWork.Pages.ContactModel
@{
}
<div id="plsCenter">
<form method="post" class="contact-form col-12 form-outline">
    <div class="text-left">
    <label class="form-label" for="name">Name</label>
    <input class="col-md-4 col-sm-6" id="name" type="text" />
    <br />
    <label class="form-label" for="email">E-mail </label>
    <input class="col-md-4 col-sm-6" id="email" type="email" />
    </div>
    <br />
    <label class="form-label" for="textarea">Message</label>
    <textarea id="textarea" class="form-control w-100" rows="6" maxlength="250" spellcheck="false"></textarea>
    <br />
    <input id="submit" type="submit" class="btn btn-primary" value="Done" />
</form>
</div>